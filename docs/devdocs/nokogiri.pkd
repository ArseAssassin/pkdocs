       */
      sibling = node->prev;
      if (!sibling) { return Qnil; }

      while (sibling && sibling->type != XML_ELEMENT_NODE) {
        sibling = sibling->prev;
      }

      return sibling ? noko_xml_node_wrap(Qnil, sibling) : Qnil ;
    }
