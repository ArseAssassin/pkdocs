    static VALUE
    new (int argc, VALUE *argv, VALUE klass)
    {
      xmlDocPtr xml_doc;
      xmlNodePtr node;
      VALUE document;
      VALUE name;
      VALUE content;
      VALUE rest;
      VALUE rb_node;

      rb_scan_args(argc, argv, "3*", &document, &name, &content, &rest);

