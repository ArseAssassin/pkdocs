    # => [#(Element:0x35c { name = "div", children = [ #(Text "Start")] }),
    #     #(Element:0x370 {
    #       name = "a",
    #       attributes = [ #(Attr:0x384 { name = "href", value = "#jump" })],
    #       children = [ #(Text "shortcut")]
