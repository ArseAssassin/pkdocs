      default: '1'
      rest: false
      kind: kw-only
    - name: sock
      default: None
      rest: false
      kind: kw-only
    - name: backlog
      default: '100'
      rest: false
      kind: kw-only
    - name: ssl
      default: None
      rest: false
      kind: kw-only
    - name: reuse_address
      default: None
      rest: false
      kind: kw-only
    - name: reuse_port
      default: None
      rest: false
      kind: kw-only
    - name: ssl_handshake_timeout
      default: None
      rest: false
      kind: kw-only
    - name: ssl_shutdown_timeout
      default: None
      rest: false
      kind: kw-only
    - name: start_serving
      default: 'True'
